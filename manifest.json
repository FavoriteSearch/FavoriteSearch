{
  "manifest_version": 3,
  "name": "Favorite Search",
  "version": "1.0",
  "description": "Search your browser favorites/bookmarks and display matching results",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
